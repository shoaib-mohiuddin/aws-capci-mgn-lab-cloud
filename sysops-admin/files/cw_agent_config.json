{
    "agent": {
      "metrics_collection_interval": 60
    },
    "metrics": {
      "namespace": "CapciMgnPhpmyAdmin",
      "metrics_collected": {
        "cpu": {
          "resources": [
            "*"
          ],
          "measurement": [
            "cpu_usage_idle",
            "cpu_usage_iowait",
            "cpu_usage_user",
            "cpu_usage_system"
          ],
          "totalcpu": false,
          "metrics_collection_interval": 60
        },
        "disk": {
          "resources": [
            "*"
          ],
          "measurement": [
            {"name": "free", "rename": "DISK_FREE", "unit": "Gigabytes"},
            "total",
            "used",
            "used_percent",
            "inodes_free"
          ],
          "metrics_collection_interval": 60
        },
        "diskio": {
          "resources": [
            "*"
          ],
          "measurement": [
            "reads",
            "writes",
            "read_time",
            "write_time",
            "read_bytes",
            "write_bytes",
            "io_time"
          ],
          "metrics_collection_interval": 60
        },
        "swap": {
          "measurement": [
            "swap_used",
            "swap_free",
            "swap_used_percent"
          ]
        },
        "mem": {
          "measurement": [
            "mem_free",
            "mem_used",
            "mem_cached",
            "mem_used_percent",
            "mem_total"
          ],
          "metrics_collection_interval": 60
        },
        "net": {
          "resources": [
            "*"
          ],
          "measurement": [
            "bytes_sent",
            "bytes_recv",
            "drop_in",
            "drop_out"
          ]
        },
        "netstat": {
          "measurement": [
            "tcp_established",
            "tcp_syn_sent",
            "tcp_close"
          ],
          "metrics_collection_interval": 60
        },
        "processes": {
          "measurement": [
            "running",
            "sleeping",
            "dead",
            "total",
            "idle"
          ]
        }
      },
      "append_dimensions": {
        "InstanceId": "${aws:InstanceId}"
      },
      "aggregation_dimensions" : [["InstanceId"]],
      "force_flush_interval" : 60
    }
  }